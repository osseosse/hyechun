<script>
	function drawImage(){

	    var sel = document.getElementById("color");
	    
	    for(var i=0; i<sel.length; i++) 
	    {
	        if(sel[i].selected == true) 
	        {
	            
	            document.getElementById(sel[i].value).style.display="block";
	        }
	        else{
	        	document.getElementById(sel[i].value).style.display="none";
	        }
	    }
	}
</script>

